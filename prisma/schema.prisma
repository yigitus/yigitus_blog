generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
}

model Post {
  id        String     @default(cuid()) @id
  title     String
  content   String?
  published Boolean    @default(false)
  slug      String     @unique
}